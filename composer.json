{
    "name": "henrotaym/laravel-package-versioning-config",
    "description": "Versioning your package while registering common features.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Henrotay",
            "email": "mathieu.henrotay@gmail.com"
        }
    ],
    "require": {
        "henrotaym/laravel-test-suite": "^3.0",
        "henrotaym/laravel-helpers": "^2.0",
        "henrotaym/laravel-container-auto-register": "^1.2 || ^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Henrotaym\\LaravelPackageVersioning\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Henrotaym\\LaravelPackageVersioning\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Henrotaym\\LaravelPackageVersioning\\Providers\\LaravelPackageVersioningServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "2.x.x-dev"
        }
    }
}
